{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "mydatafactorypractice123"
		},
		"AzureBlobStorage1_linkedservice_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1_linkedservice'"
		},
		"AzureSqlDatabaselinkedservice_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabaselinkedservice'"
		},
		"AzureSqlDatabaselinkedservice_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "jyothisainniserver.database.windows.net"
		},
		"AzureSqlDatabaselinkedservice_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "jyothisainnidb"
		},
		"AzureSqlDatabaselinkedservice_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "jyothisainniadmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "mydatafactorypractice123_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_linkedservice_connectionString": {
						"value": "[parameters('AzureBlobStorage1_linkedservice_connectionString')]"
					},
					"AzureSqlDatabaselinkedservice_password": {
						"value": "[parameters('AzureSqlDatabaselinkedservice_password')]"
					},
					"AzureSqlDatabaselinkedservice_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabaselinkedservice_properties_typeProperties_server')]"
					},
					"AzureSqlDatabaselinkedservice_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabaselinkedservice_properties_typeProperties_database')]"
					},
					"AzureSqlDatabaselinkedservice_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabaselinkedservice_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}